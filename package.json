    "test": "mocha --require should-approximately-deep --reporter mocha-better-spec-reporter --recursive"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mljs/pca.git"
  },
  "keywords": [
    "pca",
    "principal",
    "component",
    "analysis",
    "dimensionality",
    "reduction",
    "data",
    "mining",
    "datamining",
    "machine",
    "learning"
  ],
  "author": "Jefferson Hern√°ndez",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mljs/pca/issues"
  },
  "homepage": "https://github.com/mljs/pca",
  "dependencies": {
    "ml-matrix": "^1.1.5",
    "ml-stat": "^1.1.0"
  },
  "devDependencies": {
    "documentation": "^4.0.0-beta5",
    "ml-dataset-iris": "^1.0.0",
    "mocha": "^2.3.4",
    "mocha-better-spec-reporter": "^3.0.1",
    "should": "^9.0.0",
    "should-approximately-deep": "^1.1.0"
