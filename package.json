{
  "name": "ml-pca",
  "version": "1.1.1",
  "description": "Principal component analysis",
  "main": "src/pca.js",
  "directories": {
    "lib": "src",
    "test": "test"
  },
  "files": [
    "src"
  ],
  "scripts": {
    "build-doc": "documentation build --github --output doc --format html",
